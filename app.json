{
  "expo": {
    "android": {
      "package": "com.ghost.quotesapp",
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "name": "QuotesApp",
    "slug": "QuotesApp",
    "version": "1.0.3",
    "description": "Create, customize and share beautiful inspirational quotes with stunning backgrounds and smooth animations.",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "quotesapp",
    "userInterfaceStyle": "automatic",
    "primaryColor": "#667eea",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cf321c09-0403-4033-bc8e-3cb2eeffa361"
      }
    },
    "owner": "bishnuf66"
  }
}
